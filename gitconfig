[color]
    ui = true
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    pager = true
[alias]
    graph = log --graph --all --decorate --color
    ls = log --decorate --color
    co = checkout
    br = branch
    ci = commit
    st = status
    files = log --name-only
    lg = log --graph --all --pretty=format:'%C(bold)%h%Creset -%C(auto)%d%Creset %s %C(green dim)(%cr)%Creset %C(ul)<%an>'
    lognotag = log --format='%C(auto) %h%x09%an%x09%cd%x09%s'
    g = grep --break --heading --line-number
[core]
    editor = vim
    whitespace = fix
[grep]
    lineNumber = true
    extendRegexp = true
[pull]
    rebase = true
[init]
    defaultBranch = master

